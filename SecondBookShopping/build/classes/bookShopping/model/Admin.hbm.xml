<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping  >

<!--   类和关系的映射文件 -->
	<class name="bookShopping.model.Admin" table="bookshopping_admin" lazy="false">
		<id name="id" column="id" >
			<generator class="identity"/>
		</id>
		<property name="adminname" column="adminname" length="20"/>
		<property name="adminpassword" column="adminpassword" length="20"/>
	</class>


</hibernate-mapping>
